<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".SettingFragment">

    <LinearLayout
        android:id="@+id/linearLayoutTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/backArrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="@null"
            android:src="@drawable/back"
            app:tint="@color/black" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/textviewtitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/montserrat_bold"
            android:text="Setting"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="32sp" />
    </LinearLayout>
<!--    <LinearLayout-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:orientation="vertical"-->
<!--        android:layout_marginTop="30dp"-->
<!--        app:layout_constraintTop_toBottomOf="@id/linearLayoutTitle">-->

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewProfileSettings"
        android:text="\t\tProfile Settings"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:fontFamily="@font/montserrat_regular"
        android:layout_gravity="center"
        android:layout_marginTop="40dp"
        android:paddingTop="4dp"
        android:drawableStart="@drawable/user"
        android:drawableTint="@color/yellow"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/linearLayoutTitle"
        android:layout_marginEnd="16dp"
        tools:ignore="MissingConstraints" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:id="@+id/line1"
        android:background="#AFAFAF"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewProfileSettings"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewAccountSettings"
        android:text="\t\tAccount Settings"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:fontFamily="@font/montserrat_regular"
        android:layout_gravity="center"
        android:layout_marginTop="26dp"
        android:paddingTop="4dp"
        android:drawableStart="@drawable/ic_baseline_settings_24"
        android:drawableTint="@color/yellow"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/line1"
        android:layout_marginEnd="16dp"
        tools:ignore="MissingConstraints" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:id="@+id/line2"
        android:background="#AFAFAF"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewAccountSettings"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewSubscription"
        android:text="\t\tSubscription"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:fontFamily="@font/montserrat_regular"
        android:layout_gravity="center"
        android:layout_marginTop="26dp"
        android:paddingTop="4dp"
        android:drawableStart="@drawable/subscription"
        android:drawableTint="@color/yellow"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/line2"
        android:layout_marginEnd="16dp"
        tools:ignore="MissingConstraints" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:id="@+id/line3"
        android:background="#AFAFAF"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewSubscription"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewAbout"
        android:text="\t\tAbout"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:fontFamily="@font/montserrat_regular"
        android:layout_gravity="center"
        android:layout_marginTop="26dp"
        android:paddingTop="4dp"
        android:drawableStart="@drawable/about"
        android:drawableTint="@color/yellow"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/line3"
        android:layout_marginEnd="16dp"
        tools:ignore="MissingConstraints" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:id="@+id/line4"
        android:background="#AFAFAF"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewAbout"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewFAQ"
        android:text="\t\tFAQ"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:fontFamily="@font/montserrat_regular"
        android:layout_gravity="center"
        android:layout_marginTop="26dp"
        android:paddingTop="4dp"
        android:drawableStart="@drawable/about"
        android:drawableTint="@color/yellow"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/line4"
        android:layout_marginEnd="16dp"
        tools:ignore="MissingConstraints" />


    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:id="@+id/line5"
        android:background="#AFAFAF"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/textViewFAQ"/>

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPrivacyPolicy"
        android:text="Privacy Policy"
        android:layout_marginStart="32dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/montserrat_regular"
        android:textSize="14sp"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/line5"
        tools:ignore="MissingConstraints" />
    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewTermsAndConditions"
        android:text="Terms &amp; Conditions"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_regular"
        android:textSize="14sp"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/textViewPrivacyPolicy"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textview.MaterialTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewLogout"
        android:text="Logout"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/montserrat_regular"
        android:textSize="14sp"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/textViewTermsAndConditions"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>